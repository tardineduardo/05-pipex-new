	"version": "0.2.0",
	"configurations": [
		{
			"name": "(gdb) Iniciar",
			"type": "cppdbg",
			"request": "launch",
			"program": "/nfs/homes/eduribei/Documentos/repos/05-pipex-new/pipex",
			"args": ["Makefil", "ls", "wc", "cat -e", "outout.txt"],
			"stopAtEntry": false,
			"cwd": "/nfs/homes/eduribei/Documentos/repos/05-pipex-new/",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Habilitar a reformatação automática para gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				},
				{
					"description": "Definir Tipo de Desmontagem como Intel",
					"text": "-gdb-set disassembly-flavor intel",
					"ignoreFailures": true
				}
			]
		}

	]
